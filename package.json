{
	"name": "ts-utilities",
	"version": "0.0.0-development",
	"description": "A collection of Typescript utilities",
	"main": "index.js",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "rollup -c",
		"compile": "tsc",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
		"lint:fix": "prettier-eslint 'src/**/*.{js,jsx,ts,tsx,scss,css,json,md,html}' --write",
		"prepare": "husky install",
		"semantic-release": "semantic-release"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Norgate-AV-Holdings-Ltd/ts-utilities.git"
	},
	"keywords": [
		"ts",
		"js",
		"typescript",
		"javascript",
		"utilities"
	],
	"author": "Norgate AV Holdings Ltd",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Norgate-AV-Holdings-Ltd/ts-utilities/issues"
	},
	"homepage": "https://github.com/Norgate-AV-Holdings-Ltd/ts-utilities#readme",
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.4",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-typescript": "^7.10.4",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"babel-jest": "^26.3.0",
		"commitizen": "^4.2.4",
		"coveralls": "^3.1.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^7.28.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jest": "^23.20.0",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^6.0.0",
		"jest": "^26.4.2",
		"lint-staged": "^11.0.0",
		"prettier": "^2.3.1",
		"prettier-eslint": "^12.0.0",
		"prettier-eslint-cli": "^5.0.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.51.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-terser": "^7.0.1",
		"rollup-plugin-ts": "^1.4.0",
		"semantic-release": "^17.4.3",
		"typescript": "^4.3.2"
	},
	"lint-staged": {
		"*.{js, jsx, ts, tsx, md, html, css}": [
			"yarn lint:fix"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
