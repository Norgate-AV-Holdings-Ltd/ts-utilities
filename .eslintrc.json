{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-typescript/base",
		"prettier",
		"plugin:jest/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
		"prettier",
        "@typescript-eslint",
		"jest"
    ],
    "rules": {
		"no-console": "off",
		"import/prefer-default-export": "off",
		"import/no-dynamic-require": "off"
    }
}
